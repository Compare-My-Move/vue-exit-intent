{
  "name": "vue-exit-intent",
  "version": "2.0.0",
  "description": "Exit Intent Composable. Show your content when a user is leaving, or another threshold reached.",
  "author": "nickap <dev.texng@simplelogin.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "build-spa": "run-p type-check build-only-spa",
    "preview": "vite preview",
    "build-only": "vite build",
    "build-only-spa": "vite build --mode spa",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^18.14.2",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4",
    "vite": "^3.1.8",
    "vite-plugin-dts": "^2.2.0",
    "vue-tsc": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickap/vue-exit-intent.git"
  },
  "keywords": [
    "exit",
    "intent",
    "vue",
    "use",
    "vueExitIntent",
    "composable",
    "mouse",
    "leave",
    "modal",
    "popup",
    "pop-up",
    "vue3"
  ],
  "bugs": {
    "url": "https://github.com/nickap/vue-exit-intent/issues"
  },
  "homepage": "https://github.com/nickap/vue-exit-intent#readme"
}
